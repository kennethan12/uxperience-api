{
  "name": "uxperience",
  "version": "1.0.0",
  "description": "uxperience",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "npm run build:dist8 && npm run build:dist10",
    "build:apidocs": "lb-apidocs",
    "build:current": "lb-tsc",
    "build:dist8": "lb-tsc es2017",
    "build:dist10": "lb-tsc es2018",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist*",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run prettier:fix && npm run tslint:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build:current",
    "test": "lb-mocha --allow-console-logs \"DIST/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs DIST/test/**/*.js && npm run posttest",
    "prestart": "npm run build:current",
    "start": "node .",
    "prepublishOnly": "npm run test",
    "live": "db-migrate up && node .",
    "ken": "NODE_ENV=dev && export DB_HOST=localhost && export DB_PORT=3306 && export DB_USER=root && export DB_PASSWORD= && export DB_DATABASE=uxperience && npm start",
    "miki": "export DB_HOST=ix-fs-1.ce5d5ftkvwyr.eu-west-1.rds.amazonaws.com && export DB_PORT=3306 && export DB_USER=ix_fs_1 && export DB_PASSWORD=ixperience2018 && export DB_DATABASE=localhost && npm start"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist*/src",
    "dist*/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^0.11.5",
    "@loopback/context": "^0.11.9",
    "@loopback/core": "^0.10.1",
    "@loopback/dist-util": "^0.3.3",
    "@loopback/openapi-v3": "^0.10.12",
    "@loopback/repository": "^0.12.1",
    "@loopback/rest": "^0.15.1",
    "@types/bcrypt": "^2.0.0",
    "@types/bcryptjs": "^2.4.1",
    "@types/jsonwebtoken": "^7.2.8",
    "bcrypt": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "db-migrate": "^0.11.1",
    "db-migrate-mysql": "^1.1.10",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.3.0",
    "loopback-connector-mysql": "^5.2.0",
    "stripe": "^6.2.1"
  },
  "devDependencies": {
    "@loopback/build": "^0.6.11",
    "@types/node": "^10.1.1",
    "@loopback/testlab": "^0.10.10",
    "@types/mocha": "^5.0.0",
    "mocha": "^5.1.1",
    "source-map-support": "^0.5.5"
  }
}
